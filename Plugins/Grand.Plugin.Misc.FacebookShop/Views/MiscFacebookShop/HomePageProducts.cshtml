@model List<ProductOverviewModel>
@using Grand.Plugin.Misc.FacebookShop.Models;
@if (Model.Count > 0)
{
    <div class="product-grid home-page-product-grid">
        @foreach (var item in Model)
        {
            <div class="item-box">
                @Html.Partial("~/Plugins/Misc.FacebookShop/Views/MiscFacebookShop/ProductBox.cshtml", item)
            </div>
        }
    </div>
}