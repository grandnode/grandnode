@model RegisterResultModel
@inject IPageHeadBuilder pagebuilder
@{
    Layout = "_ColumnsOne";

    //title
    pagebuilder.AddTitleParts(T("PageTitle.Register").Text);

    //continue URL
    var qs = this.Context.Request.Query["ReturnUrl"];
    var continueUrl = "";
    if (!String.IsNullOrEmpty(qs) && Url.IsLocalUrl(qs))
    {
        continueUrl = qs;
    }
    else
    {
        continueUrl = Url.RouteUrl("HomePage");
    }
}
<div class="page registration-result-page text-center">
    <h2 class="generalTitle">@T("Account.Register")</h2>
    <section class="result">
        @Model.Result
    </section>
    <div class="btn-group generalMarginSupporter">
        <input type="button" class="btn btn-secondary register-continue-button" onclick="location.href='@continueUrl'" value="@T("Account.Register.Result.Continue")">
    </div>
</div>
