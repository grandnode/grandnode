@model RecurringPaymentModel

<div asp-validation-summary="All"></div>
<input asp-for="Id" type="hidden" />
<admin-tabstrip name="recurringpayment-edit">
    <items>
        <tabstrip-item text="@T("Admin.RecurringPayments.Info")" tab-index="0">
            <content>
                <div>
                    <partial name="_CreateOrUpdate.TabInfo" model="Model" />
                </div>
            </content>
        </tabstrip-item>
        <tabstrip-item text="@T("Admin.RecurringPayments.History")" tab-index="1">
            <content>
                <div>
                    <partial name="_CreateOrUpdate.TabHistory" model="Model" />
                </div>
            </content>
        </tabstrip-item>
        <vc:admin-widget widget-zone="reccuringpayment_details_tabs" additional-data="Model" />
    </items>
</admin-tabstrip>